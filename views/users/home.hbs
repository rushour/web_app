<!-- Section for Search -->
<section class="container-fluid fill" id="search">
<br><br><br><br>
  <div class="row">
  	<div class="col-sm-8 col-sm-offset-3 col-md-8 col-md-offset-3">
	    <div class="card-container manual-flip">
        <div class="card">
            <div class="front">
          		<div class="col-sm-6 col-sm-offset-3 col-md-6 col-md-offset-3">
          		  <h1 class="text-center v-center">Search</h1>
          		</div>
              <div class="content">
                <div class="row">
    	            <div class="col-sm-12 col-sm-offset-1 col-md-12 col-md-offset-1">
  	                <div class="form-group">
  	                  <div class="col-lg-10 col-sm-10 col-md-10">
  	                    <input type="text" name="searchBox" class="form-control" id="inputSearch" autocomplete="off" placeholder="Search for..."/>
												<br/>
  	                    <br>
  	                  </div>
    	              </div>
    	            </div>
      	        </div>
              </div>
            </div>
            <div class="back">
              <div class="header">
                  <h5 class="motto" id="restaurantName">Restaurant Name</h5>
              </div> 
              <div class="content">
            		<div class="container">
            		  <div class="row">
            		    <div class="col-md-4">
            		      <img src="" height="100" style="max-width: 120px" alt="" class="img-rounded" id="restaurantImage">
            		    </div>
            		    <div class="col-md-4">
            		      <blockquote>
            		        <!-- <h4 id="restaurantCity">City</h4> -->
            		        <small><cite title="Source Title" id="restaurantAddress">Gotham, United Kingdom</cite></small>
            		        <small><cite title="Source Title" id="restaurantCity">Gotham, United Kingdom</cite></small>
            		        <small><cite title="Source Title" id="restaurantCountry">Gotham, United Kingdom</cite></small>
            		      </blockquote>
            		    </div>
            		  </div>
              		</div>
                <div class="btn-group center">
                  <button class="btn btn-block btn-primary" type="submit" onclick="backToSearch(this)">Back to Search</button>
                </div>
              </div>
            </div>
        </div>
    	</div>
  	</div>
  </div>
              
</section>

