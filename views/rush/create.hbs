<br><br><br><br><br><br><br>
<div class="container">
	<div class="row">
		<div class="col-md-4 col-md-offset-4">
		  <form name="create-rushHistory-form" id="create-rushHistory-form" method="post">
		    <div class="form-group">
		      <label for="restaurantID">restaurantID</label>
		      <input type="text" id="restaurantID" name="restaurantID" required value="{{input.restaurantID}}" />
		    </div>
		    <div class="form-group">
		      <label for="rush">Rush Percentage</label>
		      <input type="text" id="rush" name="rush" required value="{{input.rush}}" />
		    </div>
		    <div class="form-group">
		      <label for="day">Day</label>
		      <input type="text" id="day" name="day" required value="{{input.day}}" />
		    </div>
		    <div class="form-group">
		      <label for="year">Year</label>
		      <input type="text" id="year" name="year" required value="{{input.year}}" />
		    </div>
		    <div class="form-group">
		      <label for="month">Month</label>
		      <input type="text" id="month" name="month" required value="{{input.month}}" />
		    </div>
		    <div class="form-group">
		      <label for="date">Date</label>
		      <input type="text" id="date" name="date" required value="{{input.date}}" />
		    </div>
		    <div class="form-group">
		      <label for="hour">Hours</label>
		      <input type="text" id="hours" name="hours" required value="{{input.hours}}" />
		    </div>
		    <div class="form-group">
		      <label for="minutes">Minutes</label>
		      <input type="text" id="minutes" name="minutes" required value="{{input.minutes}}" />
		    </div>
		    <div class="form-group">
		      <label for="seconds">Seconds</label>
		      <input type="text" id="seconds" name="seconds" required value="{{input.seconds}}" />
		    </div>
		    <div class="form-submit">
		      <input type="submit" value="Create my account" />
		    </div>
		    <span style="color:red;">{{error}}</span>
		  </form>
		</div>
	</div>
</div>